/* pages/cart/cart.wxss */
.content{
  position: fixed;
  height: 100%;
  width: 100%;
  background-color: #FAFAFA;
  display: flex;
 
}
.goods{
  flex: 1;
  overflow-y: scroll;
 
}


.payment{
  position: absolute;
  background-color: white;
  height: 80rpx;
  width: 100%;
  bottom: 0;
  display: flex;
  flex-wrap: wrap;
  justify-content: center;
  overflow: hidden;
  padding: 20rpx;
  border-top: 1rpx solid #eee;
}
.payment>checkbox{
  flex: 1;
  font-size:30rpx; 
  text-align: center;
  line-height: 80rpx;
}
.payment>.total{
  flex: 3;
  font-size: 28rpx;
  display: flex;
  flex-wrap: wrap;
  flex-direction: column;
  
}
.total>.price{
  flex: 1;
  text-align: center;
 
}
.price>text:nth-child(2){
  color: red;
}
.total>.info{
  flex: 1;
  display: block;
  font-size: 24rpx;
  color: #CFCFCF;
  text-align: center;
}

.accounts{
  flex: 1;
  display: block;
  background-color: #835F41;
  color: white;
  text-align: center;
  line-height: 80rpx;
  border-radius: 8rpx;
  font-size: 28rpx;
  margin-right: 40rpx;
}
